CREATE TABLE PARA BIGQUERY

CREATE TABLE `proyecto.dataset.cliprod_complemento_otros` (
  id_migracion INT64,
  id_ingesta INT64,
  CUITCLIE INT64,
  GRUPOGENERAL STRING,
  TIPOPERD STRING,
  SEXOCODI STRING,
  ESTCIVIL STRING,
  EDAD INT64,
  PROVINCIA STRING,
  LOCALIDAD STRING,
  FEC_ALTA DATE,
  CANAL_ALTA STRING,
  USR_RETAIL_UM STRING,
  USR_CORPORATE_UM STRING,
  BANCON_EMP_UM STRING,
  USA_BANCON_U3M BOOLEAN,
  INGRESOS_UM NUMERIC,
  INGR_PROM_U6M NUMERIC,
  CALCULO_ING STRING,
  NRO_EMP STRING,
  CON_INGR BOOLEAN,
  CAPITA STRING,
  ES_ESTUDIANTE BOOLEAN,
  ES_JUB_PENS BOOLEAN,
  CTA_TJ_SOCIAL BOOLEAN,
  fecha_carga TIMESTAMP,
  PERIODO DATE
)
PARTITION BY DATE(fecha_carga)
CLUSTER BY CUITCLIE, PROVINCIA;
